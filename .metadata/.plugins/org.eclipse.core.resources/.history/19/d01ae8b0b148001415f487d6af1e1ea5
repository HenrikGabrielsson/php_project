<?php

namespace model;

class Answer
{
	public $pollId;	//undersökning som svaret hör till
	public $answer;	//svarstext
	public $count;		//antal röster på detta svar.
	
	public function __construct($answer, $id = 0, $pollId = 0, $count = 0)
	{
		$this->id = $id;
		$this->pollId = $pollId;
		$this->answer = $answer;
		$this->count = $count;
	} 
	
	public function getId() 
	{
		return $this->id;
	}
	
	public function getPollId()
	{
		return $this->pollId;
	}
	
	public function getAnswer()
	{
		return $this->answer;
	}
	
	public function getCount()
	{
		return $this->count;
	}	
}